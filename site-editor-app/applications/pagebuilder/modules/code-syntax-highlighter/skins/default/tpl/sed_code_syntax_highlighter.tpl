<div {{sed_attrs}} class="s-tb-sm module module-code-syntax-highlighter code-syntax-highlighter-default {{className}}" >
    <pre id="code-syntax-highlighter-{{sed_model_id}}" class="{{code_params}}" {{{title}}} >{{content}}</pre>
    <textarea class="code-syntax-highlighter-textarea" name="code-syntax-highlighter-editor-{{sed_model_id}}" id="code-syntax-highlighter-editor-{{sed_model_id}}">{{{content}}}</textarea>
    <div class="close-code-editor"><i class="close fa fa-close"></i><span class="close-text"></span></div>
</div>