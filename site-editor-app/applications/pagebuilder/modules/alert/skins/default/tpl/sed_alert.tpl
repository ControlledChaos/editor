<div {{sed_attrs}} class="sed-alert s-tb-sm module module-alert alert-skin-default  {{className}} {{type}}">
  <div class="alert alert-variant-style " role="alert">
      <button type="button" class="close" data-dismiss="alert">
          <span aria-hidden="true">&times;</span>
          <span class="sr-only">{{I18n.close}}</span>
      </button>
      <div class="alert-icons"><i class="{{icon}}"></i></div>
       {{{content}}}
  </div>
</div>          
